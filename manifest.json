{
    "name": "MuchTube",
    "version": "1.0",
    "permissions": ["storage", "tabs"],
    "manifest_version": 2,
    "description": "A youtube extension that ask's you why you watching the video",
    "action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "content-scripts": [ 
    {
        "matches": ["https://*.youtube.com/*"]
    }
    ]
}